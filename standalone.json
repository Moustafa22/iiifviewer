{
  "id": "standalone",
  "inherits": "base.json",

  "inputs": ["src/standalone.js", "ol3js/externs/olx.js"],
  "paths": ["src", "ol3js/src", "ol3js/build/ol.ext"],

  "define": {
    "goog.DEBUG": false,
    "ol.ENABLE_PROJ4JS": false,
    "ol.ENABLE_VECTOR": false,
    "ol.ENABLE_WEBGL": false,
    "ol.ENABLE_IMAGE": false
  },

  "externs": [
    "externs/jspdf.externs.js",
    "ol3js/externs/bingmaps.js",
    "ol3js/externs/geojson.js",
    "ol3js/externs/oli.js",
    "ol3js/externs/proj4js.js",
    "ol3js/externs/tilejson.js",
    "ol3js/externs/closure-compiler.js"
  ],

  "output-wrapper": "/* IiifViewer; Copyright (C) 2014 - Klokan Technologies GmbH; Powered by OpenLayers 3 */\n(function() {%output%})();"
}
